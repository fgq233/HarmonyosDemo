{
  "app": {
    "bundleName": "ohos.samples.jsapplication",
    "version": {
      "code": 2000000,
      "name": "2.0"
    }
  },
  "deviceConfig": {},
  "module": {
    "package": "ohos.samples.jsapplication",
    "name": ".JsApplication",
    "reqCapabilities": [
      "video_support"
    ],
    "deviceType": [
      "phone",
      "tv"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "entry",
      "moduleType": "entry",
      "installationFree": false
    },
    "abilities": [
      {
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ],
        "formsEnabled": false,
        "visible": true,
        "name": "ohos.samples.jsapplication.MainAbility",
        "icon": "$media:icon",
        "description": "$string:mainability_description",
        "label": "$string:app_name",
        "type": "page",
        "launchType": "standard"
      }
    ],
    "js": [
      {
        "pages": [
          "pages/index/index"
        ],
        "name": "default",
        "window": {
          "designWidth": 720,
          "autoDesignWidth": false
        }
      }
    ]
  }
}